{

  "manifest_version": 2,
  "name": "AlienAlien",
  "version": "1.0",

  "description": "Adds a UFO to the browser",

  "icons": {
    "48": "icons/border-48.png"
  },
  
  "web_accessible_resources": [
	"images/alien.gif"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["alien.js"]
    }
  ]

}